{
  "name": "aries-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --mode dev",
    "local": "tsc && vite build --mode dev",
    "build": "tsc && vite build --mode production",
    "preview": "vite preview",
    "i18n:extract": "formatjs extract 'src/**/*.{tsx,jsx,ts,js}' --ignore 'src/vite-env.d.ts' --out-file 'lang/message.json' --throws",
    "poeditor:pull": "./command/poeditor-tools pull -c poeditor.config.json",
    "poeditor:push": "pnpm i18n:extract && ./command/poeditor-tools push -c poeditor.config.json -m 'lang/message.json' && rm -rf 'lang'",
    "poeditor:sync": "pnpm poeditor:pull && pnpm poeditor:push"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.24.2",
    "@headlessui/react": "0.0.0-insiders.8adaeed",
    "@hookform/resolvers": "^3.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.7",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react-slick": "^0.23.10",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.2",
    "event-source-polyfill": "^1.0.31",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-collapsed": "^4.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.3",
    "react-notifications-component": "^4.0.1",
    "react-paginate": "^8.2.0",
    "react-qr-code": "^2.0.11",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-slick": "^0.29.0",
    "react-table": "^7.8.0",
    "react-use": "^17.4.0",
    "slick-carousel": "^1.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@formatjs/cli": "^6.1.2",
    "@hookform/devtools": "^4.3.1",
    "@types/event-source-polyfill": "^1.0.1",
    "@types/react": "^18.2.7",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.2.4",
    "@types/react-table": "^7.7.14",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "babel-plugin-formatjs": "^10.5.2",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-i18next-scanner": "^0.4.0"
  }
}
